<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
             xmlns:nodes="clr-namespace:Navigator.UI.Models.Nodes"
             xmlns:viewModels="clr-namespace:Navigator.UI.ViewModels"
             xmlns:converters="clr-namespace:Navigator.UI.Converters"
             x:Class="Navigator.UI.Views.FileWindowTabView"
             x:DataType="viewModels:FileWindowTabViewModel"
             d:DataContext="{d:DesignInstance Type=viewModels:FileWindowTabViewModel, IsDesignTimeCreatable=False}">

    <UserControl.Resources>
        <converters:ByteArrayToBitmapConverter x:Key="ByteArrayToBitmapConverter" />
    </UserControl.Resources>

    <Grid RowDefinitions="Auto,*,Auto" Background="{StaticResource AppBackground}">
        <!-- trimmed for brevity -->
    </Grid>
</UserControl>

